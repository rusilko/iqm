<%= form_for @training do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.date_select :end_date %>
  </p>
  <p>
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :price_per_person %><br />
    <%= f.number_field :price_per_person %>
  </p>
  <p>
    <%= f.label :training_type_id %><br />
    <%= f.number_field :training_type_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>

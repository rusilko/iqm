<tr>
<%= f.hidden_field :training_id, value: t_id %>
<%= f.simple_fields_for :client, defaults: { label: false, wrapper_html: { class: '' }, input_html: { class:'' } } do |builder| %>
  <td class="no"><%= index %></td>
  <td class="name"> <%= builder.input :name %></td>
  <td class="email"><%= builder.input :email %></td>
  <td class="tel">  <%= builder.input :phone_1 %></td>
  <td class="price"></td>
  <td class="trashbin">
    <% unless index==1 %>
      <%= f.hidden_field :_destroy %><%= link_to raw('<i class="icon-trash"></i>'), '#', class: "remove_event_participation_btn"  %>
    <% end %>
  </td>
<% end -%>
<%= f.hidden_field :id %>
</tr>